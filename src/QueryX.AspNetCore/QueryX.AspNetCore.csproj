<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <Nullable>enable</Nullable>
    <Copyright>Copyright © 2022 Hector Carballo</Copyright>
    <PackageProjectUrl>https://github.com/hector-co/QueryX</PackageProjectUrl>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <PackageLicenseFile>LICENSE.txt</PackageLicenseFile>
    <Authors>Hector Carballo</Authors>
    <VersionPrefix>1.0.0</VersionPrefix>
    <VersionSuffix>alpha-0001</VersionSuffix>
    <RepositoryUrl>https://github.com/hector-co/QueryX</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\..\LICENSE.txt">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
    <None Include="..\..\README.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>
    
  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.DependencyInjection.Abstractions" Version="6.0.0" />
  </ItemGroup>
    
  <ItemGroup>
    <ProjectReference Include="..\QueryX\QueryX.csproj" />
  </ItemGroup>

</Project>
